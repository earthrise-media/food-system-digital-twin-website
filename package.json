    "classnames": "^2.3.2",
    "deck.gl": "~8.8.24",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "fs-extra": "^11.1.1",
    "google-polyline": "^1.0.3",
    "jotai": "^2.1.0",
    "jotai-location": "^0.5.1",
    "knex": "^2.4.2",
    "knex-postgis": "^0.14.3",
    "leva": "^0.9.34",
    "mapbox-gl": "3.0.0-beta.1",
    "next": "13.3.0",
